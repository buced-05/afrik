{
  "name": "ivoire-ai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:all": "node start-dev.js",
    "app": "node start-dev.js",
    "clean": "rm -rf .next node_modules/.cache .turbo",
