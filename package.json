    "@types/react-dom": "^18.2.18",
    "idb": "^8.0.0",
    "lucide-react": "^0.556.0",
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.32",
