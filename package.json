    "backend": "cd backend && python -m uvicorn app.main:app --reload",
    "backend:install": "cd backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@tensorflow/tfjs": "^4.15.0",
